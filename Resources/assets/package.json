{
  "name": "@umanit/block-collection-bundle",
  "description": "Package for umanit/block-collection-bundle",
  "license": "MIT",
  "version": "0.0.1",
  "symfony": {
    "controllers": {
      "collection": {
        "main": "dist/collection.js",
        "fetch": "lazy",
        "enabled": true
      },
      "ckeditor": {
        "main": "dist/ckeditor.js",
        "fetch": "lazy",
        "enabled": true
      },
      "crop": {
        "main": "dist/crop.js",
        "fetch": "lazy",
        "enabled": true
      },
      "media": {
        "main": "dist/media.js",
        "fetch": "lazy",
        "enabled": true
      },
      "modal": {
        "main": "dist/modal.js",
        "fetch": "lazy",
        "enabled": true
      }
    }
  },
  "scripts": {
    "build": "babel src -d dist --copy-files"
  },
  "peerDependencies": {
    "@umanit/block-bundle": "0.0.1",
    "stimulus": "^2.0.0"
  },
  "dependencies": {
    "@umanit/tools": "^1.0.3",
    "cropperjs": "^1.5.7",
    "stimulus-use": "^0.23.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "core-js": "^3.0.0",
    "stimulus": "^2.0.0"
  }
}
